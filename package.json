    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "postcss": "^8.5.1",
    "tailwindcss": "^4.0.3"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.0",
    "@types/echarts": "^4.9.22",
    "@types/node": "^22.13.1",
    "antd": "^5.23.3",
    "echarts": "^5.6.0",
    "katex": "^0.16.21",
    "react-markdown": "^9.0.3",
    "react-router-dom": "^7.1.5",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0"
  }
}
